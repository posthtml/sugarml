{
  "name": "sugarml",
  "description": "basic parser for whitespace-significant html",
  "version": "0.0.1",
  "author": "Jeff Escalante",
  "ava": {
    "verbose": "true"
  },
  "bugs": "https://github.com/posthtml/sugarml/issues",
  "devDependencies": {
    "ava": "0.15.x",
    "coveralls": "2.x",
    "nyc": "6.x",
    "posthtml": "jescalan/posthtml#parser-config",
    "standard": "7.x"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "homepage": "https://github.com/posthtml/sugarml",
  "license": "MIT",
  "main": "lib",
  "repository": "posthtml/sugarml",
  "scripts": {
    "test": "ava",
    "lint": "standard",
    "coverage": "nyc ava",
    "coveralls": "nyc --reporter=lcov ava && cat ./coverage/lcov.info | coveralls"
  }
}
